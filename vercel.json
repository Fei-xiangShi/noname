{
  "version": 2,
  "builds": [
    { "src": "noname-server.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(checkFile|readFileAsText|writeFile|getFileList|checkDir|createDir|removeDir|removeFile)", "dest": "/noname-server.js" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
